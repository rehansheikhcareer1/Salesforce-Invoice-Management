<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_After_Invoice_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Due_Date__c &lt; Invoice_Date__c</errorConditionFormula>
    <errorDisplayField>Due_Date__c</errorDisplayField>
    <errorMessage>Due date must be on or after invoice date</errorMessage>
</ValidationRule>
